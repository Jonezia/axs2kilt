{
    "_parent_entries": [ [ "^", "byname", "shell" ] ],
    "_producer_rules": [
        [ [ "compiled", "kilt_executable", "device=qaic", "bert" ], [["run"]], { "return_saved_record_entry": false }, ["shell_cmd_with_subs"] ]
    ],

    "device": "qaic",

    "lib_kilt_path": [ "^", "execute", [[
        [ "byquery", "compiled,lib_kilt" ],
        [ "get_path" ]
    ]] ],

    "loadgen_path": [ "^", "execute", [[
        [ "byquery", "compiled,loadgen_c" ],
        [ "get_path" ]
    ]] ],

    "protobuf_path": [ "^", "execute", [[
        [ "byquery", "compiled,protobuf" ],
        [ "get_path" ]
    ]] ],

    "cjson_lib_path": [ "^", "execute", [[
        [ "byquery", "lib,lib_name=cjson" ],
        [ "get_path", ["cJSON.o"] ]
     ]] ],
    "return_this_entry": [ "^^", "execute", [[
        [ "get_kernel" ],
        [ "work_collection" ],
        [ "attached_entry", [ "compiled_kilt_executable_bert_qaic", {
                "tags": [ "compiled", "kilt_executable", "bert" ],
                "file_name" : "bert",
                "device": [ "^^", "substitute", "#{device}#" ]
            } ] ],
        [ "save" ]
    ]] ],

    "in_dir": [ "^^", "execute", [[
        [ "get", "return_this_entry" ],
        [ "get_path" ],
        0,
        [ "func", "os.path.dirname" ]
    ]] ],

    "compiler_deps": "g++ -c -I../ -DCK_HOST_OS_NAME2_LINUX=1 -DCK_HOST_OS_NAME_LINUX=1 -DCK_TARGET_OS_NAME2_LINUX=1 -DCK_TARGET_OS_NAME_LINUX=1 -std=c++11 -O3 -ffast-math -march=znver2 -DENABLE_ZEN2 -DR282 -DKILT_BENCHMARK_STANDALONE_BERT -DKILT_DEVICE_QAIC -DKILT_CONFIG_FROM_JSON -DKILT_CONFIG_TRANSLATE_X -Wall -Wno-sign-compare -std=c++17 -I/opt/qti-aic/dev/inc/ -I/opt/qti-aic/dev/inc/qaic-api-common -I/install/ -I#{cjson_lib_entry.include_path}# -I#{lib_kilt_path}#/cJSON/ -I#{lib_kilt_path}#/include -I#{protobuf_path}#/install/include -I#{loadgen_path}#/include",
    "harness_o":[ "^^", "substitute", "#{compiler_deps}#  #{lib_kilt_path}#/include/benchmarks/harness/harness.cpp -o harness.o"],
    "pack_o":[ "^^", "substitute", "#{compiler_deps}# #{lib_kilt_path}#/include/benchmarks/standalone/bert/pack.cpp -o pack.o"],
    "qaicinfapi_o":[ "^^", "substitute", "#{compiler_deps}# #{lib_kilt_path}#/include/devices/qaic/api/master/QAicInfApi.cpp  -o QAicInfApi.o"],

    "shell_cmd_with_subs": [ "^^", "substitute", "#{harness_o}# && #{pack_o}# && #{qaicinfapi_o}# && g++ harness.o pack.o QAicInfApi.o #{cjson_lib_path}# -o bert -L#{loadgen_path}#/lib -lmlperf_loadgen -L#{lib_kilt_path}#/lib -lQAic -lpthread"]
}
